Private Sub imgClickCradle_Click()
Dim k As Integer
Dim j As Integer
Call PlaySound(2)

If gblnRotorsInCradle = True Then
    'lift out rotors and show rotors with label, also show box
    gblnRotorsInCradle = False
    Me.imgCradle.Picture = Me.imgCradleEmpty.Picture
    gintRotorsOnAxle = 8
    For k = 1 To 8
        Me.imgRotorOnAxe(k).Visible = True
        Me.lblRotor(k).Visible = True
        Me.lblRotor(k).Caption = Chr(gintUsedRotNr(k) + 64)
    Next k
    'set all rotors in box
    Me.picRotorBox.Enabled = True
    Me.picRotorBox.Visible = True
    Me.picRotorBox.SetFocus
    For k = 1 To 12
        Me.imgRotorInBox(k).Visible = True
        Me.lblRotorInBox(k).Visible = True
    Next k
    'clear used rotors
    For k = 1 To 8
        Me.imgRotorInBox(gintUsedRotNr(k)).Visible = False
        Me.lblRotorInBox(gintUsedRotNr(k)).Visible = False
    Next k
    Me.imgClickAxe.ToolTipText = " Remove rotor "
    Me.imgClickCradle.ToolTipText = " Place rotors in machine "
    'show box
    Else
    'put rotors back in cradle (and validate!)
    Me.imgClickAxe.ToolTipText = " Close cover "
    Me.imgClickCradle.ToolTipText = " Lift rotors out "
    If gintRotorsOnAxle = 8 Then
        If gintRotorsOnAxle <> 8 Then Exit Sub
        For k = 1 To 8
            Me.imgRotorOnAxe(k).Visible = False
            Me.lblRotor(k).Visible = False
        Next k
        gblnRotorsInCradle = True
        Me.imgCradle.Picture = Me.imgCradleFull.Picture
        Me.picRotorBox.Visible = False
        Me.picRotorBox.Enabled = False
        'validate rotors
        For k = 1 To 8
        gstrUsedRotString(k) = gintDefineRotor(gintUsedRotNr(k))
            For j = 1 To 38
            gintUsedRotor(k, j) = StringTowire(Mid(gstrUsedRotString(k), j, 1))
            Next j
        Next k
    End If
End If

End Sub

Private Sub imgOpenHandle_Click()
'open cover
If gblnSetKeyBlock = True Then Exit Sub
Call PlaySound(2)
If gblnLockOpen = True Then
    Me.imgCradle.Picture = Me.imgCradleFull.Picture
    Me.picRotorOpen.Visible = True
    gblnCoverOpen = True
