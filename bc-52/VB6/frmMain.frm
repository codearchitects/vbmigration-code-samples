Private Sub imgKey_MouseDown(Index As Integer, Button As Integer, Shift As Integer, X As Single, Y As Single)
If gblnSetKeyBlock = True Then Exit Sub
If gblnLockOpen = True Then Exit Sub
Select Case Index
Case 39
    ' code
    Me.imgKey(Index).Picture = Me.imgButtonDn.Picture
Case 40
    ' decode
    Me.imgKey(Index).Picture = Me.imgButtonDn.Picture
Case 41
    ' set
    Me.imgKey(Index).Picture = Me.imgButtonDn.Picture
Case Else
    Me.imgKey(Index).Picture = Me.imgKeyDown(Index).Picture
End Select
End Sub

Private Sub imgKey_MouseUp(Index As Integer, Button As Integer, Shift As Integer, X As Single, Y As Single)
If gblnLockOpen = True And Index > 39 And Index < 42 Then Exit Sub
Select Case Index
Case 39
    ' CL
    Me.imgKey(Index).Picture = Me.imgButtonUp.Picture
Case 40
    ' SR
    Me.imgKey(Index).Picture = Me.imgButtonUp.Picture
Case 41
    ' CD
    Me.imgKey(Index).Picture = Me.imgButtonUp.Picture
Case Else
    Me.imgKey(Index).Picture = Nothing
End Select
End Sub

Private Sub imgKey_Click(Index As Integer)
If gblnSetKeyBlock = True Then Exit Sub
If gblnLockOpen = True And Index > 39 And Index < 42 Then Exit Sub
Dim tmp As String
If gblnRotorsInCradle = True Then Exit Sub
Select Case Index
Case 39
    'code
    gblnEncodeText = True
    Me.imgLampCode.Picture = Me.imgLampRedOn.Picture
    Me.imgLampDecode.Picture = Me.imgLampGreenOff.Picture
    Call PlaySound(2)
Case 40
    'decode
    gblnEncodeText = False
    Me.imgLampCode.Picture = Me.imgLampRedOff.Picture
    Me.imgLampDecode.Picture = Me.imgLampGreenOn.Picture
    Call PlaySound(2)
Case 41
    ' SR set rotors
    Call SetRotors
    Call PlaySound(2)
Case Else
    tmp = Mid(gstrKeyboard, Index, 1)
    Call Crypto(StringTowire(tmp))
End Select
End Sub

Private Sub imgKey_DblClick(Index As Integer)
Call imgKey_Click(Index)
End Sub

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
'REM: remove '*' if you use AZERTY and want to use shift to use figures
If gblnSetKeyBlock = True Then Exit Sub
If pblnlockKeyPress = False Then
    pblnlockKeyPress = True
    End If
Select Case KeyCode
Case 107 '+numpad
    Me.imgKey(38).Picture = imgKeyDown(38).Picture
Case 187 '+
    '*'If Shift = 1 Then
        Me.imgKey(38).Picture = imgKeyDown(38).Picture
    '*'End If
Case 32, 109 'spc or - numpad
        If gblnEncodeText = True Then
            Me.imgKey(37).Picture = imgKeyDown(37).Picture
            Else
            If KeyCode <> 32 Then Me.imgKey(37).Picture = imgKeyDown(37).Picture
        End If
Case 189 ' - (37)
    '*'If Shift = 0 Then
        Me.imgKey(37).Picture = imgKeyDown(37).Picture
    '*'End If
Case 96
    'press num pag 0
    Me.imgKey(10).Picture = imgKeyDown(10).Picture
Case 97 To 105
    'press num pad 1-9
    Me.imgKey(KeyCode - 96).Picture = imgKeyDown(KeyCode - 96).Picture
Case 48
    '*'If Shift = 1 Then
        Me.imgKey(10).Picture = imgKeyDown(10).Picture
    '*'End If
Case 49 To 57
    '*'If Shift = 1 Then
        Me.imgKey(KeyCode - 48).Picture = imgKeyDown(StringTowire(KeyCode - 48)).Picture
    '*'    End If
Case Else
    If KeyCode > 64 And KeyCode < 96 Then
    'test = StringTowire(Chr(KeyCode))
        Me.imgKey(InStr(1, gstrKeyboard, Chr(KeyCode))).Picture = imgKeyDown(InStr(1, gstrKeyboard, Chr(KeyCode))).Picture
        End If
End Select

End Sub

Private Sub Form_KeyUp(KeyCode As Integer, Shift As Integer)
'REM: remove '*' if you use AZERTY and want to use shift to use figures
If KeyCode < 20 Then Exit Sub
Dim tmp As Integer
pblnlockKeyPress = False
If gstrAutoType = True Then
    If KeyCode = 27 Then
        gstrAutoType = False
        Else
        Exit Sub
    End If
End If
Select Case KeyCode
Case 116
    'clipboard
    frmClipBoard.Show (vbModal)
    Exit Sub
Case 117
    'autotyping
    frmQuick.Show (vbModal)
    Exit Sub
Case 119
    'show key settings
    frmKeySheet.Show (vbModal)
Case 46
    'delete tape text
    Me.lblTapeInput.Caption = ""
    Me.lblTapeOutput.Caption = ""
    gstrClipInput = ""
    gstrClipOutput = ""
    GroupsCount = 0
Case 107 '+numpad
    Call Crypto(StringTowire("+"))
    Me.imgKey(38).Picture = Nothing
Case 187 '+
    '*'If Shift = 1 Then
        Call Crypto(StringTowire("+"))
        Me.imgKey(38).Picture = Nothing
    '*'End If
Case vbKeySpace, 109   'spc or - numpad
        If gblnEncodeText = True Then
            Call Crypto(StringTowire("-"))
            Else
            If KeyCode <> 32 Then Call Crypto(StringTowire("-"))
        End If
        Me.imgKey(37).Picture = Nothing
Case 189 ' -
    '*'If Shift = 0 Then
        Call Crypto(StringTowire("-"))
        Me.imgKey(37).Picture = Nothing
    '*'End If
Case 96
    'press num pag 0
    Call Crypto(10)
    Me.imgKey(10).Picture = Nothing
Case 97 To 105
    'press num pad 1-9
    Call Crypto(KeyCode - 96)
    Me.imgKey(KeyCode - 96).Picture = Nothing
Case 48
    '*'If Shift = 1 Then
        Call Crypto(10)
        Me.imgKey(10).Picture = Nothing
    '*'End If
Case 49 To 57
    '*'If Shift = 1 Then
        Call Crypto(KeyCode - 48)
        Me.imgKey(KeyCode - 48).Picture = Nothing
    '*'End If
Case Else
    If KeyCode > 64 And KeyCode < 96 Then
        Call Crypto(StringTowire(Chr(KeyCode)))
        Me.imgKey(InStr(1, gstrKeyboard, Chr(KeyCode))).Picture = Nothing
        End If
End Select
End Sub

