Private Sub SetKeyBlock()
Me.lblUnused.Caption = ""
Dim k As Integer
For k = 1 To 38
    Call SetCable(k, 0, 0)
    Call SetCable(k, StringTowire(Mid(gstrKeyBlock, k, 1)), 1)
    lblplug(k).Caption = Mid(gstrWireString, k, 1) & " " & Mid(gstrKeyBlock, k, 1)
    lblplug(k).BackColor = &HC000& 'green
Next
pstrtmpKeyBlock = gstrKeyBlock
End Sub

Private Sub picKeyBlock_KeyDown(KeyCode As Integer, Shift As Integer)
Dim Ret As Integer
Dim k As Integer
Select Case KeyCode
Case 13
    'enter (keyblock ok)
     imgKeyBlockBack_Click
Case 46
    If pblnClickAgain = True Then
        'delete current plug
        Mid(pstrtmpKeyBlock, pintFocusNr, 1) = " "
        Me.lblplug(pintFocusNr).BackColor = colEmpty
        Call SetCable(pintFocusNr, 0, 0)
        pblnClickAgain = False
        Call RefreshTmpKey
        Else
        ' DEL delet all plug
        Ret = MsgBox("Delete all current key block connections?", vbQuestion + vbYesNo, " ATOMIX")
        If Ret = vbNo Then Exit Sub
        pstrtmpKeyBlock = "                                      "
        Me.lblUnused.Caption = "1234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ-+"
        For k = 1 To 38
            lblplug(k).Caption = Mid(gstrWireString, k, 1) & "  "
            Me.lblplug(k).BackColor = colEmpty
            Call SetCable(k, 0, 0)
        Next
    End If
    Exit Sub
Case 36
    'HOME (streaght forward connections)
    Ret = MsgBox("Connect all wires straightforward?", vbQuestion + vbYesNo, " ATOMIX")
    pstrtmpKeyBlock = "1234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ-+"
    For k = 1 To 38
        Me.lblplug(k).BackColor = colOK
    Next k
    Call RefreshTmpKey
    Exit Sub
End Select
End Sub

Private Sub picKeyBlock_KeyPress(KeyAscii As Integer)
Dim wChar As String
Dim Wire As Integer
Dim wPos As Integer
Dim k As Integer
Dim Ret As Integer
Dim aChar As String
aChar = UCase(Chr(KeyAscii))
Select Case aChar
Case Chr(27)
    ' ESC cancel entries
    If pblnClickAgain = True Then
        'cancel focus
        pblnClickAgain = False
        If Mid(pstrtmpKeyBlock, pintFocusNr, 1) <> " " Then
            Me.lblplug(pintFocusNr).BackColor = colOK
            Else
            Me.lblplug(pintFocusNr).BackColor = colEmpty
        End If
        Else
        'quit settings
        Ret = MsgBox("Cancel current key block changes?", vbQuestion + vbYesNo + vbDefaultButton2, " ATOMIX")
        If Ret = vbNo Then Exit Sub
        Me.picKeyBlock.Visible = False
        Me.picKeyBlock.Enabled = False
        Me.imgKeyBlock.Visible = False
        Me.imgKeyBlock.Enabled = False
        gblnSetKeyBlock = False
    End If
Case Else
    Wire = StringTowire(aChar)
End Select
If Wire = 0 Then Exit Sub

If pblnClickAgain = False Then
    'first click
    pblnClickAgain = True
    'focus label
    Me.lblplug(Wire).BackColor = colFocus
    pintFocusNr = Wire
    
    Else
    'second click
    pblnClickAgain = False
    Me.lblplug(pintFocusNr).Caption = Mid(gstrWireString, pintFocusNr, 1) & " " & Mid(gstrWireString, Wire, 1)
    wPos = InStr(1, pstrtmpKeyBlock, WireToString(Wire))
    If wPos = 0 Then
        '2nd sign not used
        'set connection
        Mid(pstrtmpKeyBlock, pintFocusNr, 1) = WireToString(Wire)
        Call SetCable(pintFocusNr, Wire, 1) '>>>>>>>>>
        Me.lblplug(pintFocusNr).BackColor = colOK
        Else
        '2nd pos already used
        'clear used one
        Mid(pstrtmpKeyBlock, wPos, 1) = " "
        Me.lblplug(wPos).BackColor = colEmpty
        Call SetCable(wPos, 0, 0) '>>>>>>>>>
        'set connection
        Mid(pstrtmpKeyBlock, pintFocusNr, 1) = WireToString(Wire)
        Me.lblplug(pintFocusNr).BackColor = colOK
        Call SetCable(pintFocusNr, Wire, 1)  '>>>>>>>>>
    End If
End If
Call RefreshTmpKey
End Sub

Private Sub lblplug_Click(Index As Integer)
Call picKeyBlock_KeyPress(Asc(WireToString(Index)))
End Sub

Private Sub RefreshTmpKey()
Dim k As Integer
Me.lblUnused.Caption = ""
For k = 1 To 38
    If Mid(pstrtmpKeyBlock, k, 1) = " " Then
    lblplug(k).Caption = Mid(gstrWireString, k, 1) & "  "
    Call SetCable(k, 0, 0)
    Else
    lblplug(k).Caption = Mid(gstrWireString, k, 1) & " " & Mid(pstrtmpKeyBlock, k, 1)
    Call SetCable(k, StringTowire(Mid(pstrtmpKeyBlock, k, 1)), 1)
    End If
    If InStr(1, pstrtmpKeyBlock, Mid(gstrWireString, k, 1)) = 0 Then Me.lblUnused.Caption = Me.lblUnused.Caption & Mid(gstrWireString, k, 1)
Next
End Sub

Private Sub imgKeyBlockBack_Click()
If InStr(1, pstrtmpKeyBlock, " ") Then Exit Sub
gstrKeyBlock = pstrtmpKeyBlock
Me.picKeyBlock.Visible = False
Me.picKeyBlock.Enabled = False
Me.imgKeyBlock.Visible = False
Me.imgKeyBlock.Enabled = False
gblnSetKeyBlock = False
Call PlaySound(2)
End Sub

Private Sub SetCable(ByVal wire1 As Integer, ByVal wire2 As Integer, Mode As Integer)
'set/clear connection line
If Mode = 0 Then
    'clear connection
    Cable(wire1).Visible = False
    Else
    'set connection
    Cable(wire1).X1 = P1(wire1, 1)
    Cable(wire1).Y1 = P1(wire1, 2)
    Cable(wire1).X2 = P2(wire2, 1)
    Cable(wire1).Y2 = P2(wire2, 2)
    Cable(wire1).Visible = True
End If
End Sub

