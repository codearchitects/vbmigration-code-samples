Private Sub imgClickAxe_Click()
Dim k As Integer
Call PlaySound(2)
If gblnRotorsInCradle = False Then
    If tmpRotorNr = 0 Then
        'lift out a rotor and transfer to rotor box
        If gintRotorsOnAxle < 1 Then Exit Sub
        k = 9 - gintRotorsOnAxle
        Me.imgRotorOnAxe(k).Visible = False
        Me.lblRotor(k).Visible = False
        tmpRotorNr = gintUsedRotNr(k)
        Me.lblRotorOut.Caption = Chr(64 + tmpRotorNr)
        Me.picRotorOut.Visible = True
        gintRotorsOnAxle = gintRotorsOnAxle - 1
        tmpRotorPins = gstrUsedPins(k)
        Me.imgClickAxe.ToolTipText = " Put rotor on axle "
        Call ShowPins
        Else
        'put rotor on axle
        If gintRotorsOnAxle = 8 Then Exit Sub
        If InStr(1, tmpRotorPins, "0") = 0 And gintRotorsOnAxle <> 1 And gintRotorsOnAxle <> 6 Then
            'no pins deactivated, so refuse
            MsgBox "Please deactivate at least one pin to obtain secure encryption!", vbExclamation, "ATOMIX"
            Exit Sub
            End If
        If InStr(1, tmpRotorPins, "1") = 0 And gintRotorsOnAxle <> 1 And gintRotorsOnAxle <> 6 Then
            'no pins deactivated, so refuse
            MsgBox "Please activate at least one pin to obtain secure encryption!", vbExclamation, "ATOMIX"
            Exit Sub
            End If
        Me.picRotorOut.Visible = False
        gintRotorsOnAxle = gintRotorsOnAxle + 1
        k = 9 - gintRotorsOnAxle
        gintUsedRotNr(k) = tmpRotorNr
        Me.imgRotorOnAxe(k).Visible = True
        Me.lblRotor(k).Visible = True
        Me.lblRotor(k).Caption = Chr(64 + tmpRotorNr)
        gstrUsedPins(k) = tmpRotorPins
        tmpRotorNr = 0
        Me.imgClickAxe.ToolTipText = " Remove rotor "
        End If
    Else
    'close cover if rotors in cradle
    gblnRotorsInCradle = False
    gblnCoverOpen = False
    Me.picRotorOpen.Visible = False
    End If

End Sub

