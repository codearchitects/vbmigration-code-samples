    'set pin
    Wire = InStr(1, gstrWireString, UCase(Chr(KeyAscii)))
    If Wire = 0 Then Exit Sub
    Call imgPin_Click(Wire)
    Me.lblPinChoice.Caption = UCase(Chr(KeyAscii))
    Else
    'select rotor
    rotA = Asc(UCase(Chr(KeyAscii))) - 64
    If rotA < 1 Or rotA > 12 Then Exit Sub
    If Me.imgRotorInBox(rotA).Visible = True Then Call imgRotorInBox_Click(rotA)
End If
End Sub

Private Sub picRotorBox_KeyDown(KeyCode As Integer, Shift As Integer)
If Me.imgRotorOut.Visible = False Then Exit Sub
Dim r As Integer
If KeyCode = 46 Then
    r = MsgBox("Do you want to reset all pins on this rotor?", vbQuestion + vbYesNo, " ATOMIX")
    If r = vbYes Then
        tmpRotorPins = "11111111111111111111111111111111111111"
        Call ShowPins
    End If
End If
If KeyCode = 45 Then
    'INS (put on axle)
    Call imgClickAxe_Click
    Exit Sub
    End If
If KeyCode = 36 Then
    'HOME (rotor into box)
    Call imgRotorBox_Click
    Exit Sub
    End If
End Sub

